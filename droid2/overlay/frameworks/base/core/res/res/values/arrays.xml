<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Defines the set of colors used for selecting a random
          notification color. -->
    <string-array name="notification_led_random_color_set" translatable="false">
        <item>green</item>
        <!-- Droid 2 can't display a white LED for some reason.
             It comes out looking pink.
        <item>white</item>
        -->
        <item>red</item>
        <item>blue</item>
        <item>#45ff00</item>
        <item>cyan</item>
        <item>#800080</item>
        <item>#ffc0cb</item>
        <item>#ffa500</item>
        <item>#00c9ff</item>
    </string-array>

</resources>